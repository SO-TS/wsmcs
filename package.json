    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.0.0",
    "itty-router": "^4.2.0",
    "postcss": "^8.0.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.0.0",
    "terser": "^5.44.1",
    "vite": "^7.1.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^8.0.3",
    "wrangler": "^4.54.0"
