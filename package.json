{
  "name": "untitled3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && node scripts/generate-manifest.js",
    "preview": "vite preview --host",
    "optimize:images": "node scripts/optimize-images-sharp.js",
